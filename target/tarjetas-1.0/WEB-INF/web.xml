<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControladorTarjetas</servlet-name>
        <servlet-class>Controlador.ControladorTarjetas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>Controlador.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador2</servlet-name>
        <servlet-class>Controlador.Controlador2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorBuscar</servlet-name>
        <servlet-class>Controlador.ControladorBuscar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorBuscar2</servlet-name>
        <servlet-class>Controlador.ControladorBuscar2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExportController</servlet-name>
        <servlet-class>Utilidades.ExportController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorRemito</servlet-name>
        <servlet-class>Controlador.ControladorRemito</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorTarjetas</servlet-name>
        <url-pattern>/ControladorTarjetas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador2</servlet-name>
        <url-pattern>/Controlador2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorBuscar</servlet-name>
        <url-pattern>/ControladorBuscar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorBuscar2</servlet-name>
        <url-pattern>/ControladorBuscar2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportController</servlet-name>
        <url-pattern>/ExportController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorRemito</servlet-name>
        <url-pattern>/ControladorRemito</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
